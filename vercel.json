{
  "builds": [
    {
      "src": "hospital_system.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "hospital_system.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}